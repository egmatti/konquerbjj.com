<!doctype html>
<!--[if IE 9]> <html class="ie9 no-js supports-no-cookies" lang="{{ shop.locale }}"> <![endif]-->
<!--[if (gt IE 9)|!(IE)]><!--> <html class="no-js supports-no-cookies" lang="{{ shop.locale }}"> <!--<![endif]-->
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width,initial-scale=1">
  <meta name="theme-color" content="{{ settings.color_accent }}">
  <script src="https://use.fontawesome.com/ab03b8820e.js"></script>
  <link href="https://fonts.googleapis.com/css?family=Archivo+Narrow" rel="stylesheet">
  <style>

    @font-face {
    font-family: "Avenir Next Ultra Light";
    src: url("{{ '../assets/fonts/avenir-next-ultra-light.eot' | asset_url }}") format("eot"); /* IE9 Compat Modes */
    src: url("{{ '../assets/fonts/avenir-next-ultra-light.eot?#iefix' | asset_url }}") format("embedded-opentype"), /* IE6-IE8 */
        url("{{ '../assets/fonts/avenir-next-ultra-light.woff2' | asset_url }}") format("woff2"), /* Super Modern Browsers */
        url("{{ '../assets/fonts/avenir-next-ultra-light.woff' | asset_url }}") format("woff"), /* Pretty Modern Browsers */
        url("{{ '../assets/fonts/avenir-next-ultra-light.ttf' | asset_url }}") format("truetype"), /* Safari, Android, iOS */
        url("{{ '../assets/fonts/avenir-next-ultra-light.svg' | asset_url }}") format("svg"); /* Legacy iOS */
        @media screen and (-webkit-min-device-pixel-ratio:0) {@font-face {font-family: 'Avenir Next Ultra Light'; src: url("{{ '../assets/fonts/avenir-next-ultra-light.svg' | asset_url }}") format('svg');}}
    }

    @font-face {
    font-family: "Avenir Next Regular";
    src: url("{{ '../assets/fonts/avenir-next-regular.eot' | asset_url }}") format("eot"); /* IE9 Compat Modes */
    src: url("{{ '../assets/fonts/avenir-next-regular.eot?#iefix' | asset_url }}") format("embedded-opentype"), /* IE6-IE8 */
        url("{{ '../assets/fonts/avenir-next-regular.woff2' | asset_url }}") format("woff2"), /* Super Modern Browsers */
